<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         id="WebApp_ID" version="2.5">
    <display-name>web-peptide-shaker-1.0</display-name>

    <listener>
        <listener-class>com.uib.web.peptideshaker.listeners.VaadinSessionListener</listener-class>
    </listener>
    <listener>
        <listener-class>com.uib.web.peptideshaker.listeners.VaadinContextListener</listener-class>
    </listener>

    <!-- Session Configuration -->
    <session-config>
        <session-timeout>15</session-timeout>
    </session-config>
  
   

</web-app>